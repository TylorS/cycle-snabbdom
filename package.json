  "devDependencies": {
    "@cycle/core": "5.x.x",
    "babel": "^5.8.23",
    "babelify": "^6.4.0",
    "browserify": "^11.2.0",
    "eslint": "^1.0.0",
    "eslint-config-cycle": "^3.0.0",
    "eslint-plugin-cycle": "^1.0.1",
    "eslint-plugin-no-class": "^0.1.0",
    "rx": "^4.0.6",
